<!-- Modal -->
<div class="modal fade" id="deleteBookingModal{{this.id}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Booking deletion</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        Are you sure you want to delete booking:<br>
        <h2>{{this.vendor.companyName}} </h2><br>
        <h3>{{this.location.country}}</h3><br>
        <h5>{{this.location.city}} {{this.location.postalCode}}</h5><br>
        <h5>{{this.location.streetName}} {{this.location.streetNumber}}</h5><br>
        <h5>{{dateFrom}} - {{dateTO}}</h5>
      </div>
      <form action="/bookings/delete/{{this.id}}" method="post">
        <div class="modal-footer">
          <div class="col align-self-end">
            <button type="button" class="btn btn-secondary btn-sm float-left" data-dismiss="modal">Cancel</button>
            <input class="btn btn-primary btn-sm float-right" type="submit" value="Submit">
          </div>
        </div>
      </form>
    </div>
  </div>
</div>
